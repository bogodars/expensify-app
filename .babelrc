{
  "env": {
    "development": {
      "presets": ["env", "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    },
    "production": {
      "presets": ["env", "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    },
    "test": {
      "presets": ["@babel/env", "@babel/react"],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  }
}
